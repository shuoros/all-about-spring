<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Item.
    -->
    <changeSet id="20230220085321-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="item"
                                 constraintName="fk_item__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="basket_id"
                                 baseTableName="item"
                                 constraintName="fk_item__basket_id"
                                 referencedColumnNames="id"
                                 referencedTableName="basket"/>
    </changeSet>
</databaseChangeLog>
